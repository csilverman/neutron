{
  "name": "imageblog",
  "version": "0.1.0",
  "private": true,
  "type": "module",

"scripts": {
    "clean": "rimraf _site .cache",
  
    "build": "npm run build:eleventy && npm run build:sass",
    "build:eleventy": "npx @11ty/eleventy",
    "build:sass": "npx sass src/assets/scss/style.scss _site/assets/css/style.css --no-source-map",
  
    "start": "npm run dev",
  
    "dev": "run-p dev:*",
    "dev:eleventy": "npx @11ty/eleventy --serve",
    "dev:sass": "npx sass --watch --poll src/assets/scss/style.scss:_site/assets/css/style.css --no-source-map"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^5.0.0",
    "luxon": "^3.7.2",
    "sharp": "^0.34.5",
    "sass": "^1.80.0",
    "npm-run-all": "^4.1.5"

  },
  "devDependencies": {
    "rimraf": "^6.1.2",
    "sass": "^1.97.1"
  },
  "description": "",
  "main": ".eleventy.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
